{
  "default_level": "info",
  "contexts": {
    "main": {
      "enabled": true,
      "level": "warn",
      "file": "logs/main.log",
      "max_size_mb": 50,
      "max_files": 5,
      "console": true
    },
    "compiler": {
      "enabled": true,
      "level": "debug",
      "file": "logs/compiler.log",
      "max_size_mb": 50,
      "max_files": 5,
      "console": false
    }
  }
}